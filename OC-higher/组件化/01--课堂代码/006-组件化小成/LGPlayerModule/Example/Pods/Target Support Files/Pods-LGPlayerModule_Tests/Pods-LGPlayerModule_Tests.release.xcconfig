CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
ENABLE_BITCODE = NO
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AFNetworking" "${PODS_CONFIGURATION_BUILD_DIR}/CocoaAsyncSocket" "${PODS_CONFIGURATION_BUILD_DIR}/JSONModel" "${PODS_CONFIGURATION_BUILD_DIR}/LGCommonUIModule" "${PODS_CONFIGURATION_BUILD_DIR}/LGDLNAModule" "${PODS_CONFIGURATION_BUILD_DIR}/LGMacroAndCategoryModule" "${PODS_CONFIGURATION_BUILD_DIR}/LGPlayerModule" "${PODS_CONFIGURATION_BUILD_DIR}/MRDLNA" "${PODS_CONFIGURATION_BUILD_DIR}/Masonry"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/AFNetworking/AFNetworking.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/CocoaAsyncSocket/CocoaAsyncSocket.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/JSONModel/JSONModel.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LGCommonUIModule/LGCommonUIModule.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LGDLNAModule/LGDLNAModule.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LGMacroAndCategoryModule/LGMacroAndCategoryModule.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/LGPlayerModule/LGPlayerModule.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/MRDLNA/MRDLNA.framework/Headers" "${PODS_CONFIGURATION_BUILD_DIR}/Masonry/Masonry.framework/Headers" ${SDKROOT}/usr/include/libxml2
OTHER_LDFLAGS = $(inherited) -l"c++" -l"icucore" -l"xml2" -l"z" -framework "AFNetworking" -framework "CFNetwork" -framework "CocoaAsyncSocket" -framework "CoreGraphics" -framework "Foundation" -framework "JSONModel" -framework "LGCommonUIModule" -framework "LGDLNAModule" -framework "LGMacroAndCategoryModule" -framework "LGPlayerModule" -framework "MRDLNA" -framework "Masonry" -framework "MobileCoreServices" -framework "Security" -framework "SystemConfiguration" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
